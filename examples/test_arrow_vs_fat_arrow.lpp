#pragma paradigm hybrid

// Test both -> and => syntax for lambdas

fn double(x: int) -> int {
    return x * 2;
}

fn main() -> int {
    // Arrow syntax: -> (with explicit return type inference)
    let nums1 = [1, 2, 3, 4, 5];
    let doubled1 = nums1 @ (x -> double(x));
    
    // Fat arrow syntax: => (shorthand, more functional style)
    let nums2 = [1, 2, 3, 4, 5];
    let doubled2 = nums2 @ (x => double(x));
    
    // Both should work identically
    return 0;
}
